<h2>
    Search Results
</h2>

<ol>
    {{#each restaurants}}
    <li id="restaurant">
        <a href="/restaurantInfo/{{this.id}}">{{this.restaurant_name}}</a>
    </li>

    <p>{{this.city}}, {{this.state}}</p>
    {{/each}}
</ol>

<h2>Didn't find what you were looking for?</h2>
<h3>
<a href="/addRestaurant">
    submit a new restaurant
</a>
</h3>

